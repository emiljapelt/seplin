external funny_time(int c) {
    while(!(c = 0)) {
        print funny;
        c := c - 1;
    }
}

external fib(int c) {
    int res;
    'fib(res, 1, c);
    print res;
}

internal 'fib(int a, int b, int c) {
    if (c <= 0) stop;
    var temp := a;
    a := b;
    b := temp + b;
    'fib(a, b, c-1);
}