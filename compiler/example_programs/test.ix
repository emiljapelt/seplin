external calls(int amount) {
    until (amount < 0) 'calls(amount);
}

internal 'calls(int a) {
    a := a - 1;
}


internal fbrea(locked int ls, locked int b) {
    for(int i := 0; i < ls; i := i+1) {
        if (i = b) break;
        print funny+i;
    }
}

internal fcont(locked int ls, locked int c) {
    for(int i := 0; i < ls; i := i+1) {
        if (i = c) continue;
        print funny+i;
    }
}

