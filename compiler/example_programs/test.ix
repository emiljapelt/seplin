external fib(int c) {
    int res;
    'fib(res, 1, c);
    print res;
}

internal 'fib(int a, int b, int c) {
    if (c <= 0) stop;
    var temp := a;
    a := b;
    b := temp + b;
    'fib(a, b, c-1);
}



internal add(locked int x, locked int y, int r) {
    r := x + y;
}

internal mul(locked int x, locked int y, int r) {
    r := x * y;
}

internal addmul2(locked int x, locked int y, int r)
    .add(x,y,r)
    .mul(r,2,r)